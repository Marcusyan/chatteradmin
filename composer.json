{
    "name": "marcusyan/chatteradmin",
    "description": "A package to provide extended functionality for the beautiful thedevdojo/chatter forums for Laravel",
    "homepage": "https://github.com/codiiv/chatteradmin",
    "keywords": [
        "chatter",
        "laravel",
        "laravel forum",
        "laravel forum admin",
        "chatter admin"
    ],
    "require": {
        "devdojo/chatter": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Codiiv",
            "email": "codiivdev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Codiiv\\Chatter\\": "/src"
        }
      },
      "extra": {
          "laravel": {
              "providers": [
                  "Codiiv\\Chatter\\ChatterServiceProvider"
              ]
          }
      },
      "minimum-stability": "stable",
      "require": {
        "laravel/framework": "5.*"
      },
      "require-dev": {
          "laravel/laravel": "5.*"
    }
