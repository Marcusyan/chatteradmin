{
    "name": "Keagang/chatteradmin",
    "description": "A package to provide extended functionality for the beautiful thedevdojo/chatter forums for Laravel",
    "homepage": "https://github.com/Keagang/chatteradmin",
    "keywords": [
        "chatter",
        "laravel",
        "laravel forum",
        "laravel forum admin",
        "chatter admin"
    ],
    "require": {
        "devdojo/chatter": "^0.2.6"
    },
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Keagang/chatteradmin"
        }
      ],
    "autoload": {
        "psr-4": {
          "Codiiv\\Chatter\\": "/src"
        }
      },
      "extra": {
          "laravel": {
              "providers": [
                  "Codiiv\\Chatter\\ChatterServiceProvider"
              ]
          }
      },
      "minimum-stability": "dev",
      "require": {
        "laravel/framework": "5.8.*"
      },
      "require-dev": {
          "laravel/laravel": "5.8."
    }
}

